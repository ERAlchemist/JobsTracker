<!DOCTYPE html>
<html>
  <head>
    <title>JobTracker</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="css/uikit.min.css" />
        <script src="js/uikit.min.js"></script>
        <script src="js/uikit-icons.min.js"></script>
  </head>

  <body>
    <nav class="uk-navbar-container" >
      <div class="uk-navbar-center" style="background-color: #EEE">
        <ul class="uk-navbar-nav">
          <li><%= link_to 'Prospects', prospects_path %><li>
          <li><%= link_to 'Jobs Applied For', jobs_path %><li>
        </ul>
        <hr>
      </div>
       
    </nav>
   
    <%= yield %>
  
  </body>
</html>
